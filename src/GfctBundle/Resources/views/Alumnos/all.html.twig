<!DOCTIPE html>
<html>
  <head>
    <link href="{{asset('/css/styleGfct.css')}}" rel="stylesheet">
  </head>
  <body class="cuerpo">
    <h1>GESTOR FCT SECCION ALUMNOS</h1>
    <table id="alumnos">
      <tr>
        <th>#</th>
        <th>Nombre</th>
        <th>Apellido 1</th>
        <th>Apellido 2</th>
        <th>Ciclo</th>
        <th>Nombre empresa</th>
        <th>Nombre profesor</th>
      </tr>
    {% for item in alumnos %}
      <tr>
        <td>{{ item.id }}</td>
        <td>{{ item.nombre }}</td>
        <td>{{ item.apellido1 }}</td>
        <td>{{ item.apellido2 }}</td>
        <td>{{ item.ciclo }}</td>
        <td>{{ item.nEmpresa.nombre }}</td>
        <td>{{ item.nProfesor.nombre }}</td>
      <tr>
    {% endfor %}
    </table>
  </body>
</html>
